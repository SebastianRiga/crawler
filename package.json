{
  "name": "crawler",
  "version": "0.0.0001",
  "description": "A simple dungeon crawler written in typescript using phaser and parcel.",
  "repository": "https://sebastianrigadevelopment@dev.azure.com/sebastianrigadevelopment/Crawler/_git/Crawler",
  "author": "Sebastian Riga",
  "license": "MIT",
  "private": true,
  "scripts": {
    "init": "yarn install",
    "develope": "parcel src/index.html -p 3000",
    "build": "parcel build src/index.html --dist-dir dist",
    "clear": "rm -rf node_modules dist .parcel-cache",
    "clear:dist": "rm -rf dist",
    "clear:parcel_cache": "rm -rf .parcel-cache",
    "clear:node_modules": "rm -rf node_modules"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "eslint": "^7.15.0",
    "parcel": "^2.0.0-beta.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "phaser": "^3.24.1"
  },
  "parcelCleanPaths": [
    "dist"
  ],
  "staticFiles": {
    "staticPath": "assets",
    "watcherGlob": "**"
  }
}
