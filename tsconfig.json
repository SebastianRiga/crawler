{
    "compileOnSave": true,
    "compilerOptions": {
      "baseUrl": "./",
      "outDir": "./dist/out-tsc",
      "allowSyntheticDefaultImports": true,
      "resolveJsonModule": true,
      "sourceMap": true,
      "declaration": false,
      "downlevelIteration": true,
      "experimentalDecorators": true,
      "moduleResolution": "node",
      "importHelpers": true,
      "target": "es2015",
      "module": "es2020",
      "lib": [
        "es2020",
        "dom",
        "ScriptHost"
      ]
    }
  }